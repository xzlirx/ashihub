sw1ndlernotify = loadstring(game:HttpGet('https://raw.githubusercontent.com/benomat/scripts/m/paste/sw1ndlernotify.lua'))()
function OLDnotify(t)
    game:GetService("StarterGui"):SetCore("SendNotification", {
		Title = "xzlirx's Hub";
		Text = t})
end
local msgb = loadstring(game:HttpGet('https://raw.githubusercontent.com/xHeptc/NotificationGUI/main/source.lua'))()
function AskLoad(t)
    msgb.Show({Position = UDim2.new(0.5,0,0.5,0), Text = "xzlirx's Hub", Description = t, MessageBoxIcon = "Question", MessageBoxButtons = "YesNo", Result = function(res)
        if (res == "Yes") then
            return true
        else return false
        end
     end})
    end
function fn(t,d,duration)
    fuckdude({Description = d;Title = t;Duration = duration;})
end
function sAsk(t,s)
    sw1ndlernotify:CreatePromptNotif({
        TweenSpeed = 1,
        Title = "xzlirx's Hub",
        Text = t,
        Duration = d,
        Callback = function(Value)
          if Value then loadstring(s)() end
        end
       })
    end
function sNotify(t,d)
    sw1ndlernotify:CreateDefaultNotif({
        TweenSpeed = 1,
        Title = "xzlirx's Hub",
        Text = t,
        Duration = d
       })
    end
